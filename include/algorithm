#ifndef LIBBMCXX_ALGORITHM_INCLUDED
#define LIBBMCXX_ALGORITHM_INCLUDED 1

namespace std {

template <typename T>
constexpr const T& max(const T &a, const T &b) { return a < b ? b : a; }

template <typename T>
constexpr const T& min(const T &a, const T &b) { return a < b ? a : b; }

} // namespace std

#endif
